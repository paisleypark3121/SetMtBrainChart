<UserControl x:Class="SetMyBrainWPFChart.FacebookUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SetMyBrainWPFChart"
             mc:Ignorable="d" 
             d:DesignHeight="40" d:DesignWidth="160">
    <Grid>
        <Button x:Name="Login" Content="Login" HorizontalAlignment="Left" VerticalAlignment="Center" Width="75" Click="Login_Click" />
        <Button x:Name="Logout" Content="Logout" HorizontalAlignment="Right" VerticalAlignment="Center" Width="75" Click="Logout_Click"/>



        <!--<WebBrowser x:Name="SocialBrowser" ></WebBrowser>-->
    </Grid>
</UserControl>
