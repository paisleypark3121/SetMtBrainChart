<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <appSettings>
    <add key="COM" value="COM3" />
    <add key="visibility_limit" value="150"/>
    
    <!-- 
    mock -> test purposes
    real -> real life purposes 
    -->
    <add key="startup" value="mock" />
    <add key="tag" value="smartman"/>

    <!-- 
    true -> log files in a new folder created in the exe folder named with the current date and with 3 splitted files with the TAG prefix
    false -> real life purposes 
    -->
    <add key="log" value="false"/>

    <!-- 
    normal -> standard windows application
    overlay -> overlay mode
    -->
    <add key="mode" value="overlay" />

    <!-- 
    normal
    smart
    -->
    <add key="collector" value="smart" />

    <!-- 
    true
    false
    -->
    <add key="Alpha1" value="false"/>
    <add key="Alpha2" value="false"/>
    <add key="Beta1" value="false"/>
    <add key="Beta2" value="false"/>
    <add key="Gamma1" value="false"/>
    <add key="Gamma2" value="false"/>
    <add key="Delta" value="false"/>
    <add key="Theta" value="false"/>
    <add key="Attention" value="false"/>
    <add key="Creativity" value="false"/>
    <add key="Immersion" value="false"/>
    <add key="Arousal" value="false"/>
    <add key="Engagement" value="false"/>
    <add key="SlopeTheta" value="true"/>
    <add key="SlopeBeta" value="true"/>
    <add key="SlopeAlpha" value="true"/>
    <add key="SlopePower" value="false"/>
    
  </appSettings>
</configuration>