<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <appSettings>
    <add key="COM" value="COM3" />
    <add key="visibility_limit" value="150"/>
    
    <!-- 
    mock -> test purposes
    real -> real life purposes 
    -->
    <add key="startup" value="real" />
    <add key="tag" value="WherIsMyMind"/>

    <!-- 
    true -> log files in a new folder created in the exe folder named with the current date and with 3 splitted files with the TAG prefix
    false -> real life purposes 
    -->
    <add key="log" value="true"/>

    <!-- 
    normal -> standard windows application
    overlay -> overlay mode
    -->
    <add key="mode" value="normal" />
  </appSettings>
</configuration>